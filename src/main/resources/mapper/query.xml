<?xml version="1.0" encoding="UTF-8" ?>
<!-- Mybatis config sample -->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SelectMapper">

    <select id="selectAllCity" resultType="City">
        SELECT ID
             ,NAME
             ,COUNTRY
             ,POPULATION
        FROM CITY
    </select>

</mapper>